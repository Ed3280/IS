@model IntranetWeb.ViewModel.Servicio.SolicitudMantenimiento
@{ViewBag.Title = "Solicitudes de mantenimiento";}
<div class="row">
    <div class="col-xs-12">
        <div class="table-header">
            @ViewBag.Title &nbsp;
            <div class="widget-toolbar"> </div>
        </div>
        <!-- div.table-responsive -->
        <!-- div.dataTables_borderWrap -->
        <div id="dynamic-table_wrapper" class="dataTables_wrapper form-inline no-footer">
            <table id="tablaSolicitudServicio" class="table table-striped table-bordered table-hover dataTable no-footer DTTT_selectable"
                   data-url="@Url.Action("consultarSolicitudesMantenimiento")"                   
                   data-url-localization="@Url.Content("~/Scripts/dataTables/localization/"+Resources.ConfiguracionResource.DataTables+ ".json")"
                     data-url-consultar="@Url.Action("_getConsultarServicio")"                   
                   role="grid" aria-describedby="dynamic-table_info">
                <thead>
                    <tr role="row">
                        <th tabindex="0" aria-controls="dynamic-table" data-name="EditarRegistro" rowspan="1" colspan="1" aria-label="Acciones"></th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="Documento" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.Documento)">@Html.DisplayNameFor(m => m.Documento)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="NombreCliente" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.NombreCliente)">@Html.DisplayNameFor(m => m.NombreCliente)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="TipoMantenimiento" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.TipoMantenimiento)">@Html.DisplayNameFor(m => m.TipoMantenimiento)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="FechaSolicitud" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.FechaSolicitud)">@Html.DisplayNameFor(m => m.FechaSolicitud)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="Vehiculo" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.Vehiculo)">@Html.DisplayNameFor(m => m.Vehiculo)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="Kilometraje" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.Kilometraje)">@Html.DisplayNameFor(m => m.Kilometraje)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="Dispositivo" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.Dispositivo)">@Html.DisplayNameFor(m => m.Dispositivo)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="EstatusSolicitud" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.EstatusSolicitud)">@Html.DisplayNameFor(m => m.EstatusSolicitud)</th>
                        <th class="sorting" tabindex="0" aria-controls="dynamic-table" data-name="FechaEstatus" rowspan="1" colspan="1" aria-label="@Html.DisplayNameFor(m => m.FechaEstatus)">@Html.DisplayNameFor(m => m.FechaEstatus)</th>                        
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>
